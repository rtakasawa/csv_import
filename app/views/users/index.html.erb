<%= form_tag import_users_path, multipart: true do %>
  <%= file_field_tag :file %>
  <%= submit_tag "インポート" %>
<% end %>


<h1>ユーザー一覧</h1>

<table>
  <thead>
  <tr>
    <th>ID</th>
    <th>名前</th>
    <th>年齢</th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.id %></td>
      <td><%= user.name %></td>
      <td><%= user.age %></td>
    </tr>
  <% end %>
  </tbody>
</table>
